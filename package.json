{
	"name": "code-transformer",
	"description": "Code Transformer - Transform files and repositories using Gemini 1.5 Pro",
	"version": "0.0.1",
	"publisher": "jasondel",
	"private": true,
	"engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "codeTransformer.promptView",
					"name": "Calico Colors"
				}
			]
		},
		"commands": [
			{
				"command": "codeTransformer.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "codeTransformer.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "codeTransformer.clearColors",
					"group": "navigation",
					"when": "view == codeTransformer.promptView"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.32.0",
		"typescript": "^5.4.2"
	}
}
